{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch",
      "program": "${workspaceRoot}/node_modules/pxt-core/built/pxt.js",
      "stopOnEntry": false,
      "args": [
        "serve"
      ],
      "runtimeExecutable": null,
      "runtimeArgs": [
        "--nolazy"
      ],
      "console": "integratedTerminal",
      "sourceMaps": false,
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    }
  ]
}